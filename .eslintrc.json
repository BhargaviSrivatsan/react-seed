{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "globals": {
    "window": true,
    "document": true
  },
  "rules": {
    "jsx-a11y/label-has-for": 0,
    "no-else-return": ["error", { "allowElseIf": true }],
    "no-unused-expressions": 0,
    "operator-linebreak": ["error", "after"]
  }
}